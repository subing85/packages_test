		setParent ..;
		separator -height 10 -style "in" ;

		rowColumnLayout -nc 9 -cw 1 65 -cw 2 45 -cw 3 30 -cw 4 30 -cw 5 85 -cw 6 50 -cw 7 30  -cw 8 30 -cw 9 30 ;

		text -label "Shape Size :" ;
		floatField -minValue -1000 -maxValue 1000 -value 1.00 -precision 2 -step .01 shapeSizefloatField ;
		symbolButton -h 20 -image "SETEDADDCMD.xpm" -c sumShapeSize_b ;
		symbolButton -h 20 -image "SETEDREMOVECMD.XPM" -c subtractShapeSize_b ;

		text -label "   Shape Rotate :" ;
		floatField -minValue -360 -maxValue 360 -value 90.00 -precision 2 -step .01 shapeRotatefloatField ;
		button -label "X" -align "center" -h 20 -bgc $butC[0] $butC[1] $butC[2] -c "shapeRotateX_b" ;
		button -label "Y" -align "center" -h 20 -bgc $butC[0] $butC[1] $butC[2] -c "shapeRotateY_b" ;
		button -label "Z" -align "center" -h 20 -bgc $butC[0] $butC[1] $butC[2] -c "shapeRotateZ_b" ;
		setParent ..;
		separator -height 10 -style "in" ;

		rowColumnLayout -nc 3 -cw 1 130 -cw 2 135 -cw 3 130 ;

		button -label "Reset" -align "center" -h 25 -bgc $butC[0] $butC[1] $butC[2] -c "buildCtrl_Reset" ;
		button -label "Apply" -align "center" -h 25 -bgc $butC[0] $butC[1] $butC[2] -c "buildCtrl_Apply" ;
		button -label "Close" -align "center" -h 25 -bgc $butC[0] $butC[1] $butC[2] -c "deleteUI sgControlMakerW" ;

		setParent ..;
		separator -height 10 -style "in" ;
		
	
		rowColumnLayout -nc 2 -cw 1 120 -cw 2 275 ;
		text -l "subing85@yahoo.co.in"  -al "left" ;
		text -l "Copyright ©2011 Subin. Gopi - All Rights Reserved." -al "right" ;
		setParent ..;

		showWindow $sgControlMakerWin ;
	 }

	sgControlMaker ;
