	//////////////////////////////////////////////////////////////////////////////////
	//Shape Tranfer///////////////////////////////////////////////////////////////////
	global proc shapeTranfer_b()
	 {
		string $currShape 	= `optionMenu -q -v ctrlShapes_Opt` ;
		float $shapeSize	= `floatField -q -value shapeSizefloatField` ;
		int $currentColor	= `colorIndexSliderGrp  -q -value shapeColore_slider` ;	


		string $seleShape[]		= `ls -sl` ;
		int $seleSize			= `size $seleShape` ;	

		int $i ;
		for ($i=0; $i<$seleSize; $i++)
		 {	
			createDriverShape ;
				string $seleCtrl[]	= `ls -sl` ;

				//Color
				if ($currentColor>0)
				 {
					string $controlShape[]	= `listRelatives -s $seleCtrl[0]` ;

      					setAttr ($seleCtrl[0] + ".overrideEnabled") 1 ;
      					setAttr ($seleCtrl[0] + ".overrideColor") ($currentColor-1) ;
 
      					setAttr ($controlShape[0] + ".overrideEnabled") 1 ;
      					setAttr ($controlShape[0] + ".overrideColor") ($currentColor-1) ;
				 }

				//Shape Size
				setAttr ($seleCtrl[0] + ".scaleX") $shapeSize ;
				setAttr ($seleCtrl[0] + ".scaleY") $shapeSize ;
				setAttr ($seleCtrl[0] + ".scaleZ") $shapeSize ;

			if ($currShape=="Selected Shape")
			 {
				print ("Axis is not Set" + "\n") ;
			 }
			else 
			 {
				driverAxisSetup ;
			 }

			//Snap
			string $pConst[]	= `pointConstraint -offset 0 0 0 -weight 1 $seleShape[$i] $seleCtrl[0]` ;
			string $oConst[]	= `orientConstraint -offset 0 0 0 -weight 1 $seleShape[$i] $seleCtrl[0]` ;
			string $sConst[]	= `scaleConstraint -offset 1 1 1 -weight 1 $seleShape[$i] $seleCtrl[0]` ;
			delete $pConst[0] $oConst[0] $sConst[0] ;

			parent $seleCtrl[0] $seleShape[$i] ;
			makeIdentity -apply true -t 1 -r 1 -s 1 -n 0 $seleCtrl[0] ;

			string $shapeA[]	= `listRelatives -s $seleShape[$i]` ;
			int $nodeASize		= `size $shapeA` ;

			int $a ;
			for ($a=0; $a<$nodeASize; $a++)
			 { 
				delete $shapeA[$a]; 
			 }

			string $shapeB[]	= `listRelatives -s $seleCtrl[0]` ;
			int $nodeBSize		= `size $shapeB` ;

			int $b ;
			for ($b=0; $b<$nodeBSize; $b++)
			 { 
				parent -r -s $shapeB[$b] $seleShape[$i] ;
				print $shapeB[$b] ;
				rename $shapeB[$b] ($seleShape[$i] + "Shape") ;
			 }
			delete $seleCtrl[0] ; 
		 }
		select -cl ;
	 }
	//End Shape Tranfer///////////////////////////////////////////////////////////////
	//////////////////////////////////////////////////////////////////////////////////


	//////////////////////////////////////////////////////////////////////////////////
	//Driver Offset Group Setup///////////////////////////////////////////////////////
	global proc driverOffsetGroupSetup ()
	 {
		int $offsetCount	= `intField -q -value offsetGroupCount_intField` ;

		if ($offsetCount==1)
		 {
			string $groups[]	= {"_SDK", "_OffsetGroup", "_Group"} ;
			int $grpSize		= `size $groups` ;

			int $i ;
			for ($i=0; $i<$grpSize; $i++)
			 {
				string $null	= `group -em -n $groups[$i]` ;						
			 }

			int $p ;
			for ($p=0; $p<($grpSize-1); $p++)
			 {
				parent $groups[$p] $groups[$p+1] ;
			 }
			select $groups ;			
		 }
		else if ($offsetCount>1)
		 {
			string $groups[]	= {"_SDK", "_OffsetGroup", "_Group"} ;
			string $nulls[]	= {"_A_Group", "_B_Group", "_C_Group", "_D_Group", "_E_Group", "_F_Group", "_G_Group", "_H_Group", "_I_Group", "_J_Group", "_K_Group", "_L_Group",
						   "M_Group", "_N_Group", "_O_Group", "_P_Group", "_Q_Group", "_R_Group", "_S_Group", "_T_Group", "_U_Group", "_V_Group", "_W_Group", "_X_Group", "_Y_Group", "_Z_Group"};
			int $grpSize		= `size $groups` ;
			int $nullSize		= `size $nulls` ;

			int $a ;
			for ($a=0; $a<$offsetCount; $a++)
			 {
				string $null	= `group -em -n $nulls[$a]` ;
			 }

			int $b ;
			for ($b=0; $b<($offsetCount-1); $b++)
			 {
				parent $nulls[$b] $nulls[$b+1] ;										
			 }

			int $i ;
			for ($i=0; $i<$grpSize; $i++)
			 {
				string $null	= `group -em -n $groups[$i]` ;						
			 }

			int $p ;
			for ($p=1; $p<($grpSize-1); $p++)
			 {
				parent $groups[$p] $groups[$p+1] ;
			 }

			parent $groups[0] $nulls[0] ;
			parent $nulls[$offsetCount-1] $groups[$grpSize-2] ;
			select -hi $groups ;	
		 }
	 }
	//End Driver Offset Group Setup///////////////////////////////////////////////////
	//////////////////////////////////////////////////////////////////////////////////


	//////////////////////////////////////////////////////////////////////////////////
	//Build Control Apply/////////////////////////////////////////////////////////////
	global proc buildCtrl_Apply ()
	 {
		int $driverSwitch		= `checkBox -q -v driverName_checkBox` ;
		string $driverName		= `textField -q -tx driverName_textField` ;
		string $driverNameSuffix	= `textField -q -tx driverNameSuffix_textField` ;

		int $drivenSwitch		= `checkBox -q -v drivenName_checkBox` ;
		string $drivenName		= `textField -q -tx drivenName_textField` ;
		string $drivenNameSuffix	= `textField -q -tx drivenNameSuffix_textField` ;

		int $hierarchySwitch		= `checkBoxGrp	-q -v1 rigType_checkBox` ;
		int $fkSwitch			= `checkBoxGrp	-q -v2 rigType_checkBox` ;
		int $singleSwitch		= `checkBoxGrp	-q -v3 rigType_checkBox` ;

		int $parentConstSwitch	= `checkBoxGrp -q -v1 connectionTypeA_checkBoxGrp` ; 
		int $scaleConstSwitch		= `checkBoxGrp -q -v2 connectionTypeA_checkBoxGrp` ; 
		int $pointConstSwitch		= `checkBoxGrp -q -v3 connectionTypeA_checkBoxGrp` ; 
		int $parentingSwitch		= `checkBoxGrp -q -v1 connectionTypeB_checkBoxGrp` ; 
		int $connectionSwitch		= `checkBoxGrp -q -v2 connectionTypeB_checkBoxGrp` ; 
		int $orientConstSwitch	= `checkBoxGrp -q -v3 connectionTypeB_checkBoxGrp` ;
		float $shapeSize		= `floatField -q -value shapeSizefloatField` ;
		int $currentColor		= `colorIndexSliderGrp  -q -value shapeColore_slider` ;	

		string $suffix			= "_Group" ;

		string $seleDriven[]		= `ls -sl` ;
		int $seleSize			= `size $seleDriven` ;


			//Rename Driven Name 
			string $currentDriven	;

			if ($drivenSwitch==1)
			 {
				int $c ;
				for ($c=0; $c<$seleSize; $c++)
				 {
					if ($c>=9)
					 {
						$currentDriven		= `rename $seleDriven[$c] ($drivenName + "_" + ($c+1) + "_" + $drivenNameSuffix)` ;
					 }
					else if ($c<=10)
					 {					
						$currentDriven		= `rename $seleDriven[$c] ($drivenName + "_0" + ($c+1) + "_" + $drivenNameSuffix)` ;
					 }
				 }
				print (".....Driven	name is Changed to " + $drivenName + "......................." + "\n") ;
			
			 }
			else if ($drivenSwitch==0)
			 {
				print (".....Driven	name is not Change......................." + "\n") ;
			 }

		string $seleObj[]		= `ls -sl` ;

		//Hierarchy Name Quary
		string $hierarchyName ;
		if ($driverSwitch == 1)
		 {
			$hierarchyName		= $driverName ;			
		 }
		else
		 {
			$hierarchyName		= $seleObj[0] ;
		 }
		//End Hierarchy Name Quary

		//Create Hierarchy Group
		string $hierarchyGrp ;
		if  ($hierarchySwitch==1) 
		 {
			$hierarchyGrp	= `group -em -n ($hierarchyName + $suffix)` ;
		 }
		else
		 {
			print "... Hierarchy Group is not Created ..................." ;
		 }
		//End Create Hierarchy Group		
		
		int $a ;
		for ($a=0; $a<$seleSize; $a++)
		 {
			createDriverShape ;
			string $seleCtrl[]	= `ls -sl` ;

				//Color

				if ($currentColor>0)
				 {
					string $controlShape[]	= `listRelatives -s $seleCtrl[0]` ;

      					setAttr ($seleCtrl[0] + ".overrideEnabled") 1 ;
      					setAttr ($seleCtrl[0] + ".overrideColor") ($currentColor-1) ;
 
      					setAttr ($controlShape[0] + ".overrideEnabled") 1 ;
      					setAttr ($controlShape[0] + ".overrideColor") ($currentColor-1) ;
				 }

				//Shape Size
				setAttr ($seleCtrl[0] + ".scaleX") $shapeSize ;
				setAttr ($seleCtrl[0] + ".scaleY") $shapeSize ;
				setAttr ($seleCtrl[0] + ".scaleZ") $shapeSize ;

			driverAxisSetup ;

			driverOffsetGroupSetup ;
			string $seleGrps[]		= `ls -sl` ;
			int $grpSize			= `size $seleGrps` ;

			parent $seleCtrl[0] $seleGrps[0] ;

			//Snap
			select $seleObj[$a] $seleGrps[$grpSize-1] ;		
			string $pConst[]		= `pointConstraint -offset 0 0 0 -weight 1`;
			string $oConst[]		= `orientConstraint -offset 0 0 0 -weight 1`;
			string $sConst[]		= `scaleConstraint -offset 1 1 1 -weight 1`;
			delete $pConst[0] $oConst[0] $sConst[0] ;

			//Control Name Quary
			string $ctrlName ;
	
			if ($driverSwitch == 1)
			 {
				$ctrlName	= $driverName ;			
			 }
			else
			 {
				$ctrlName	= $seleObj[$a] ;
			 }

			string $offsetDriver ;
			string $driver ;

			//Rename Control And Offset Group
			if ($a>=9)
			 {
				//Rename Offset Group
				int $b ;
				for ($b=0; $b<$grpSize; $b++)
				 {
					$offsetDriver 	= `rename $seleGrps[$b] ($ctrlName + "_" +($a+1) + "_" + $driverNameSuffix + $seleGrps[$b])` ;					
				 }

				//Rename Control
 				$driver	= `rename $seleCtrl[0] ($ctrlName + "_" +($a+1) + "_" + $driverNameSuffix)` ;
			 }
			else if ($a<=10)
			 {	
				//Rename Offset Group
				int $b ;
				for ($b=0; $b<$grpSize; $b++)
				 {
					$offsetDriver 	= `rename $seleGrps[$b] ($ctrlName + "_0" +($a+1) + "_" + $driverNameSuffix + $seleGrps[$b])` ;					
				 }

				//Rename Control
 				$driver	= `rename $seleCtrl[0] ($ctrlName + "_0" +($a+1) + "_" + $driverNameSuffix)` ;
			 }

			select $driver $seleObj[$a] $offsetDriver ;
			//joint1_01_Ctrl//joint1//joint1_01_Ctrl_Group

			//Lock And Hide
			lockAndHideSwitch ;

			//Driver Connection Type

			//Parent Constraint
			if  ($parentConstSwitch==1) 
			 {
				string $seleNode[]	= `ls -sl` ;
				select $seleNode[0] $seleNode[1] ;
				string $pConst[]	= `parentConstraint -mo -weight 1 -n ($seleNode[1] + "_parentConstraint")` ;
			 }
			else
			 {
				print "Parent Constraint is not Connected" ;
			 }

			//Scale Constraint
			if  ($scaleConstSwitch==1) 
			 {
				string $seleNode[]	= `ls -sl` ;
				select $seleNode[0] $seleNode[1] ;
				string $pConst[]	= `scaleConstraint -mo -weight 1 -n ($seleNode[1] + "_scaleConstraint")` ;
			 }
			else
			 {
				print "Scale Constraint is not Connected" ;
			 }	

			//Point Constraint
			if  ($pointConstSwitch==1) 
			 {
				string $seleNode[]	= `ls -sl` ;
				select $seleNode[0] $seleNode[1] ;
				string $pConst[]	= `pointConstraint -offset 0 0 0 -weight 1 -n ($seleNode[1] + "_pointConstraint")` ;
			 }
			else
			 {
				print "Point Constraint is not Connected" ;
			 }

			//Parenting
			if  ($parentingSwitch==1) 
			 {
				string $seleNode[]		= `ls -sl` ;
				string $drivenParent[]	= `listRelatives -p $seleNode[1]` ;

				if (`objExists $drivenParent[0]`)
				 {
					if  ($fkSwitch	==1) 
					 {
						parent $seleNode[2] $drivenParent[0] ;
					 }
					else
					 {
						print "Hai Subin : Parenting" ;
					 }
				 }
				else
				 {
					print "Hai Subin : Parenting" ;
				 }
				parent $seleNode[1] $seleNode[0] ;						
			 }
			else
			 {
				print "Parenting is not Connected" ;
			 }


			//Connection
			if  ($connectionSwitch==1) 
			 {
				string $seleNode[]	= `ls -sl` ;
				string $nullGroup	= `group -em -n ($seleNode[1] + $suffix)` ; 

				//Snap
				select $seleNode[1] $nullGroup ;		
				string $pConst[]		= `pointConstraint -offset 0 0 0 -weight 1`;
				string $oConst[]		= `orientConstraint -offset 0 0 0 -weight 1`;
				string $sConst[]		= `scaleConstraint -offset 1 1 1 -weight 1`;
				delete $pConst[0] $oConst[0] $sConst[0] ;

				string $drivenParent[]	= `listRelatives -p $seleNode[1]` ;

				if (`objExists $drivenParent[0]`)
				 {
					parent $nullGroup $drivenParent[0] ;
				 }
				else
				 {
					print "Hai Subin : Connection" ;
				 }
				parent $seleNode[1] $nullGroup ;

				connectAttr -f ($seleNode[0] + ".translateX") ($seleNode[1] + ".translateX") ;
				connectAttr -f ($seleNode[0] + ".translateY") ($seleNode[1] + ".translateY") ;
				connectAttr -f ($seleNode[0] + ".translateZ") ($seleNode[1] + ".translateZ") ;

				connectAttr -f ($seleNode[0] + ".rotateX") ($seleNode[1] + ".rotateX") ;
				connectAttr -f ($seleNode[0] + ".rotateY") ($seleNode[1] + ".rotateY") ;
				connectAttr -f ($seleNode[0] + ".rotateZ") ($seleNode[1] + ".rotateZ") ;

				connectAttr -f ($seleNode[0] + ".scaleX") ($seleNode[1] + ".scaleX");
				connectAttr -f ($seleNode[0] + ".scaleY") ($seleNode[1] + ".scaleY") ;
				connectAttr -f ($seleNode[0] + ".scaleZ") ($seleNode[1] + ".scaleZ") ;
			 }
			else
			 {
				print "Connection is not Connected" ;
			 }		

			//Orient Constraint
			if  ($orientConstSwitch==1) 
			 {
				string $seleNode[]	= `ls -sl` ;
				select $seleNode[0] $seleNode[1] ;
				string $pConst[]	= `orientConstraint -offset 0 0 0 -weight 1 -n ($seleNode[1] + "_orientConstraint")` ;
			 }
			else
			 {
				print "Orient Constraint is not Connected" ;
			 }
			//End Driver Connection Type

			//Add To Hierarchy
			if ($hierarchySwitch==1) 
			 {
				if ($parentingSwitch==0)
				 {
					parent $offsetDriver $hierarchyGrp ;		
				 }
				else if ($parentingSwitch==1 && $fkSwitch==1)
				 {
					if ($a==0)
					 {
						parent $offsetDriver $hierarchyGrp ;	
					 }
				 }
				else if ($parentingSwitch==1 && $fkSwitch==0)
				 {
					parent $offsetDriver $hierarchyGrp ;	
				 }
			 }
			else
			 {
				print "...Controls are not added into Hierarchy..................." ;
			 }

			select $driver $seleObj[$a] $offsetDriver ;
			//End Add To Hierarchy
			//select -cl ;	
		 }	

		//FK Rig Connection
		if ($fkSwitch==1 && $parentingSwitch==0)
		 {			
			if ($driverSwitch == 1)
			 {
				select -r  ($driverName + "_*" + $driverNameSuffix + $suffix) ;
				string $offsetGrps[]	= `ls -sl` ;

				select -r  ($driverName + "_*" + $driverNameSuffix) ;
				string $offsetNodes[]	= `ls -sl` ;	
			
				int $offNodeSize	= `size  $offsetNodes` ;
				int $d ;
				for ($d=0; $d<($offNodeSize-1); $d++)
				 {
					string $conNode[]	= `listRelatives -c $offsetGrps[$d+1]` ;

					select -r  $offsetNodes[$d] $conNode[0] ;
					string $pConstConn[]	= `parentConstraint -mo -weight 1` ;
					string $pConstConn[]	= `scaleConstraint -mo -weight 1` ;
				 }
			 }
			else
			 {
				int $b ;
				for ($b=0; $b<($seleSize-1); $b++)
				 {
					$offsetA	= $seleObj[$b] ;
					$offsetB	= $seleObj[$b+1] ;
					select -r ($offsetB + "*" + $driverNameSuffix + $suffix) ($offsetA + "*" + ($b+1) + "_" +$driverNameSuffix) ;

					string $seleConn[]	= `ls -sl` ;
					string $conNode[]	= `listRelatives -c $seleConn[0]` ;

					select -r  $seleConn[1] $conNode[0] ;
					string $pConstConn[]	= `parentConstraint -mo -weight 1` ;
					string $pConstConn[]	= `scaleConstraint -mo -weight 1` ;
				 }
			 }
		 }
		select -cl ;
	 }
