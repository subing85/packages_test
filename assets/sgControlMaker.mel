				menuItem -l "Triangle" ;
				menuItem -l "Single Arrow" ;
				menuItem -l "Double Arrow" ;
				menuItem -l "Four Fat" ;
				menuItem -l "N Arrow on Ball" ;
				menuItem -l "S Arrow on Ball" ;
				menuItem -l "Single Pin" ;
				menuItem -l "Two Pin" ;
				menuItem -l "COG Plane" ;

				menuItem -l "   " ;

				menuItem -l "Cube" ;
				menuItem -l "Normal Sphere" ;
				menuItem -l "Smooth Sphere" ;
				menuItem -l "Normal Cylinder" ;
				menuItem -l "Smooth Cylinder" ;
				menuItem -l "Hexagon" ;
				menuItem -l "Cone" ;
				menuItem -l "Loc Shape" ;
				menuItem -l "Star Shape" ;
				menuItem -l "Selected Shape" ;

		optionMenu -l "Type:" -en 0 ctrlType_Opt ;			
				menuItem -l "Nurbs Curve" ;
				menuItem -l "Nurbs Surface" ;

		optionMenu -l "Axis:"  ctrlAxis_Opt ;
				menuItem -l "X" ;
				menuItem -l "Y" ;
				menuItem -l "Z" ;

		symbolButton -h 20 -image "OUT_RADIUSDIMENSION.xpm" -c "shapeTranfer_b" ;
		textField -tx "" -vis 0 -en 0 seletedShape_textField ;

		setParent ..;
		separator -height 10 -style "in" ;

		rowColumnLayout -nc 3 -cw 1 230 -cw 2 2 -cw 3 230 ;
		checkBoxGrp	-numberOfCheckBoxes 3
				-label "Translate ->"
				-labelArray3 "All" "Lock" "Hide" 
				-cw 1 65 -cw 2 55 -cw 3 55
				-on1 "translateAllOnSwitch"
				-of1 "translateAllOfSwitch"
				translateLH_checkBox ;

 		 button -label " " -h 10 ;

		checkBoxGrp	-numberOfCheckBoxes 3
			-label "Axis ->"
			-labelArray3 "X" "Y" "Z" 
			-cw 1 50 -cw 2 40 -cw 3 40 
			translateAxis_checkBox ;
		setParent ..;

		rowColumnLayout -nc 3 -cw 1 230 -cw 2 2 -cw 3 230 ;
		checkBoxGrp	-numberOfCheckBoxes 3
				-label "Rotate    ->"
				-labelArray3 "All" "Lock" "Hide" 
				-cw 1 65 -cw 2 55 -cw 3 55 
				-on1 "rotateAllOnSwitch"
				-of1 "rotateAllOfSwitch"
				rotateLH_checkBox ;

		button -label " " -h 10 ;

		checkBoxGrp	-numberOfCheckBoxes 3
				-label "Axis ->"
				-labelArray3 "X" "Y" "Z" 
				-cw 1 50 -cw 2 40 -cw 3 40 
				rotateAxis_checkBox ;

		setParent ..;

		rowColumnLayout -nc 3 -cw 1 230 -cw 2 2 -cw 3 230 ;
		checkBoxGrp	-numberOfCheckBoxes 3
				-label "Scale      ->"
				-labelArray3 "All" "Lock" "Hide" 
				-cw 1 65 -cw 2 55 -cw 3 55 
				-on1 "scaleAllOnSwitch"
				-of1 "scaleAllOfSwitch"
				scaleLH_checkBox ;

		button -label " " -h 10 ;

		checkBoxGrp	-numberOfCheckBoxes 3
				-label "Axis ->"
				-labelArray3 "X" "Y" "Z" 
				-cw 1 50 -cw 2 40 -cw 3 40 
				scaleAxis_checkBox ;

		setParent ..;

		rowColumnLayout -nc 3 -cw 1 230 -cw 2 2 -cw 3 230 ;
		checkBoxGrp	-numberOfCheckBoxes 3
				-label "Visibility   ->"
				-labelArray3 "All" "Lock" "Hide" 
				-cw 1 65 -cw 2 55 -cw 3 55 
				-on1 "visibilityOnSwitch"
				-of1 "visibilityOfSwitch"
				visibilityLH_checkBox ;

 		button -label " " -h 10 ;

		checkBoxGrp	-numberOfCheckBoxes 3
				-label "Axis ->"
				-labelArray3 "X" "Y" "Z" 
				-en 0
				-cw 1 50 -cw 2 40 -cw 3 40 visibilityAxis_checkBox ;
		setParent ..;
		separator -height 10 -style "in" ;


		checkBoxGrp	-numberOfCheckBoxes 3
				-label " "
				-labelArray3 "Parent Constraint" "Scale Constraint" "Point Constraint" 
				-cw 1 30 -cw 2 120 -cw 3 120  
				-on1 "parentConstOnSwitch"
				-on2 "scaleConstOnSwitch"
				-on3 "pointConstOnSwitch"
				connectionTypeA_checkBoxGrp ; 

		checkBoxGrp	-numberOfCheckBoxes 3
				-label " "
				-labelArray3 "Parenting" "Connection" "Orient Constraint " 
				-cw 1 30 -cw 2 120 -cw 3 120
				-on1 "parentingOnSwitch"
				-on2 "connectionOnSwitch"
				-on3 "orientConstOnSwitch"
				connectionTypeB_checkBoxGrp ; 

		separator -height 10 -style "in" ;

		rowColumnLayout -nc 4 -cw 1 90 -cw 2 215 -cw 3 45 -cw 4 45 ;
		checkBox -label "Driver Name :"   -align "left" driverName_checkBox ;
		textField -tx "" driverName_textField ;
		textField -tx "Ctrl" driverNameSuffix_textField ;
		button -label "Reset" -align "center" -h 20 -bgc $butC[0] $butC[1] $butC[2] -c "driverNameReset" ;
		setParent ..;

		rowColumnLayout -nc 4 -cw 1 90 -cw 2 215 -cw 3 45 -cw 4 45 ;
		checkBox -label "Driven Name :" -align "left" drivenName_checkBox ;
		textField -tx "" drivenName_textField  ;
		textField -tx "Jnt" drivenNameSuffix_textField ;
		button -label "Reset" -align "center" -h 20 -bgc $butC[0] $butC[1] $butC[2] -c "drivenNameReset" ;
		setParent ..;
		separator -height 10 -style "in" ;

		rowColumnLayout -nc 4 -cw 1 245 -cw 2 2 -cw 3 105 -cw 4 45 ;
		checkBoxGrp	-numberOfCheckBoxes 3
				//-label ""
				-labelArray3 "Hierarchy Rig" "FK Rig" "Single Rig" 
				-cw 1 100 -cw 2 65 -cw 3 70 
				-on2 "fkRigOnSwitch"
				-on3 "singleRigOnSwitch"
				rigType_checkBox ;

 		button -label " " -h 5 ;

		text -label "  Offset Group Count :" ;
   		intField -minValue -1 -maxValue 26 -value 1 offsetGroupCount_intField ;
 
		setParent ..;
		separator -height 10 -style "in" ;

		rowColumnLayout -nc 3 -cw 1 245 -cw 2 75 -cw 3 75 ;
