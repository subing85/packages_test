	//////////////////////////////////////////////////////////////////////////////////
	//Current Shape Selected//////////////////////////////////////////////////////////
	global proc currentShapeSelected ()
	 {
		string $currentShape	= `optionMenu -q -v ctrlShapes_Opt` ;
		if ($currentShape=="Selected Shape")
		 {
			string $seleObj[]	= `ls -sl` ;
			textField -e -tx $seleObj[0] seletedShape_textField ;
		 }
		else 
		 {
			textField -e -tx "" seletedShape_textField ;
		 }
	 }
	//End Current Shape Selected///////////////////////////////////////////////////////
	//////////////////////////////////////////////////////////////////////////////////


	//////////////////////////////////////////////////////////////////////////////////
	//Create Driver Shape Start///////////////////////////////////////////////////////
	global proc createDriverShape ()
	 {
		int $currentShape	= `optionMenu -q -sl ctrlShapes_Opt` ;

		switch ($currentShape)
		 { 
			//Circle		
   			case 1: 
				string $cricleNode[]		= `circle -c 0 0 0 -nr 0 1 0 -sw 360 -r 1 -d 3 -ut 0 -tol 0.01 -s 8 -ch 0 -n "ShapeCricle_Subin"` ; 
				select $cricleNode ;	
        		break ; 

			//Square
   			case 2: 	
				string $squareNodeDr		= `curve 	-d 1 -p 1 0 -1 -p -1 0 -1 -p -1 0 1 -p 1 0 1 -p 1 0 -1 -k 0 -k 1 -k 2 -k 3 -k 4` ;
				string $squareNode		= `rename $squareNodeDr "ShapeSquare_Subin"` ;
				select $squareNode ;	
        		break ; 

			//Cross
   			case 3: 
				string $crossNodeDr		= `curve 	-d 1 -p -0.2 0 0.2 -p -1 0 0.2 -p -1 0 -0.2 -p -0.2 0 -0.2 -p -0.2 0 -1 -p 0.2 0 -1 -p 0.2 0 -0.2 -p 1 0 -0.2 -p 1 0 0.2 -p 0.2 0 0.2 -p 0.2 0 1 -p -0.2 0 1 -p -0.2 0 0.2 -k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 -k 7 -k 8 -k 9 -k 10 -k 11 -k 12` ;
				string $crossNode		= `rename $crossNodeDr "ShapeCross_Subin"` ;
				select $crossNode ;			
        		break ; 

			//Triangle
   			case 4: 
				string $triangleNodeDr	= `curve 	-d 1 -p -1 0 -1 -p 0 0 1 -p 1 0 -1 -p -1 0 -1 -k 0 -k 1 -k 2 -k 3` ;
				string $triangleNode		= `rename $triangleNodeDr "ShapeTriangle_Subin"`  ;
				select $triangleNode ;	
        		break ; 

			//Single Arrow
   			case 5: 
				string $sArrowNodeDr		= `curve 	-d 1 -p -0.99 0 0 -p 0 0 1.32 -p 0.99 0 0 -p 0.33 0 0 -p 0.33 0 -0.99 -p -0.33 0 -0.99 -p -0.33 0 0 -p -0.99 0 0 -k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 -k 7` ;
				string $sArrowNode		= `rename $sArrowNodeDr "ShapeSArrow_Subin"`  ;
				select $sArrowNode ;	
        		break ; 

			//Double Arrow
   			case 6:
				string $dArrowNodeDr		= `curve	-d 1 -p -0.99 0 0.99 -p 0 0 2.31 -p 0.99 0 0.99 -p 0.33 0 0.99 -p 0.33 0 -0.99 -p 0.99 0 -0.99 -p 0 0 -2.31 -p -0.99 0 -0.99 -p -0.33 0 -0.99 -p -0.33 0 0.99 -p -0.99 0 0.99 -k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 -k 7 -k 8 -k 9 -k 10` ;
				string $dArrowNode		= `rename $dArrowNodeDr "ShapeDArrow_Subin"`  ;
				select $dArrowNode ;
        		break ; 

			//Four Fat
   			case 7:
				string $fourFatNodeDr		= `curve 	-d 1 -p -0.1485 0 -0.54675 -p -0.297 0 -0.54675 -p 0 0 -1 -p 0.297 0 -0.54675 -p 0.1485 0 -0.54675 -p 0.1485 0 -0.1485 -p 0.54675 0 -0.1485 -p 0.54675 0 -0.297 -p 1 0 0 -p 0.54675 0 0.297 -p 0.54675 0 0.1485 -p 0.1485 0 0.1485 -p 0.1485 0 0.54675 -p 0.297 0 0.54675 -p 0 0 1 -p -0.297 0 0.54675 -p -0.1485 0 0.54675 -p -0.1485 0 0.1485 -p -0.54675 0 0.1485 -p -0.54675 0 0.297 -p -1 0 0 -p -0.54675 0 -0.297 -p -0.54675 0 -0.1485 -p -0.1485 0 -0.1485 -p -0.1485 0 -0.54675 -p -0.297 0 -0.54675 -k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 -k 7 -k 8 -k 9 -k 10 -k 11 -k 12 -k 13 -k 14 -k 15 -k 16 -k 17 -k 18 -k 19 -k 20 -k 21 -k 22 -k 23 -k 24 -k 25` ;
				string $fourFatNode		= `rename $fourFatNodeDr "ShapeFourFat_Subin"`  ;
				select $fourFatNode ;
        		break ; 

			//N Arrow on Ball 
   			case 8:
				string $nArrowOnBallDr	= `curve 	-d 1 -p 0 0 -1.001567 -p -0.336638 0.327886 -0.751175 -p -0.0959835 0.327886 -0.751175 -p -0.0959835 0.500458 -0.500783 -p -0.0959835 0.604001 -0.0987656 -p -0.500783 0.500458 -0.0987656 -p -0.751175 0.327886 -0.0987656 -p -0.751175 0.327886 -0.336638 -p -1.001567 0 0 -p -0.751175 0.327886 0.336638 -p -0.751175 0.327886 0.0987656 -p -0.500783 0.500458 0.0987656 -p -0.0959835 0.604001 0.0987656 -p -0.0959835 0.500458 0.500783 -p -0.0959835 0.327886 0.751175 -p -0.336638 0.327886 0.751175 -p 0 0 1.001567 -p 0.336638 0.327886 0.751175 -p 0.0959835 0.327886 0.751175 -p 0.0959835 0.500458 0.500783 -p 0.0959835 0.604001 0.0987656 
										-p 0.500783 0.500458 0.0987656 -p 0.751175 0.327886 0.0987656 -p 0.751175 0.327886 0.336638 -p 1.001567 0 0 -p 0.751175 0.327886 -0.336638 -p 0.751175 0.327886 -0.0987656 -p 0.500783 0.500458 -0.0987656 -p 0.0959835 0.604001 -0.0987656 -p 0.0959835 0.500458 -0.500783 -p 0.0959835 0.327886 -0.751175 -p 0.336638 0.327886 -0.751175 -p 0 0 -1.001567 -k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 -k 7 -k 8 -k 9 -k 10 -k 11 -k 12 -k 13 -k 14 -k 15 -k 16 -k 17 -k 18 -k 19 -k 20 -k 21 -k 22 -k 23 -k 24 -k 25 -k 26 -k 27 -k 28 -k 29 -k 30 -k 31 -k 32` ;
				string $nArrowOnBallNode	= `rename $nArrowOnBallDr "ShapeNArrowOnBall_Subin"` ;
				select $nArrowOnBallNode ;
        		break ; 

			//S Arrow on Ball
