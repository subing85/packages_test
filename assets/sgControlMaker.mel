	//////////////////////////////////////////////////////////////////////////////////
	//Driver Axis Setup///////////////////////////////////////////////////////////////
	global proc driverAxisSetup ()
	 {
		int $axis		= `optionMenu -q -sl ctrlAxis_Opt` ;

		switch ($axis)
		 { 
   			case 1: 
				string $seleShape[]		= `ls -sl` ;
				setAttr ($seleShape[0] + ".rotateZ") 90 ;
				makeIdentity -apply true -t 1 -r 1 -s 1 -n 0 $seleShape[0] ;
        			break ; 
   			case 2: 
				print "Hai Subin..................................." ;
        			break ; 
   			case 3: 
				string $seleShape[]		= `ls -sl` ;
				setAttr ($seleShape[0] + ".rotateX") 90 ;
				makeIdentity -apply true -t 1 -r 1 -s 1 -n 0 $seleShape[0] ;
        			break ; 
		 }
	 }
	//////////////////////////////////////////////////////////////////////////////////
	//End Driver Axis Setup///////////////////////////////////////////////////////////


