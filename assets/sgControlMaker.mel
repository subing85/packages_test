	 {
		int $translateLock	= `checkBoxGrp -q -v2 translateLH_checkBox` ;
		int $translateHide	= `checkBoxGrp -q -v3 translateLH_checkBox` ;

			int $translateX	= `checkBoxGrp -q -v1 translateAxis_checkBox` ;
			int $translateY	= `checkBoxGrp -q -v2 translateAxis_checkBox` ;
			int $translateZ	= `checkBoxGrp -q -v3 translateAxis_checkBox` ;

		int $rotateLock	= `checkBoxGrp -q -v2 rotateLH_checkBox` ;
		int $rotateHide	= `checkBoxGrp -q -v3 rotateLH_checkBox` ;

			int $rotateX	= `checkBoxGrp -q -v1 rotateAxis_checkBox` ;
			int $rotateY	= `checkBoxGrp -q -v2 rotateAxis_checkBox` ;
			int $rotateZ	= `checkBoxGrp -q -v3 rotateAxis_checkBox` ;

		int $scaleLock	= `checkBoxGrp -q -v2 scaleLH_checkBox` ;
		int $scaleHide	= `checkBoxGrp -q -v3 scaleLH_checkBox` ;

			int $scaleX	= `checkBoxGrp -q -v1 scaleAxis_checkBox` ;
			int $scaleY	= `checkBoxGrp -q -v2 scaleAxis_checkBox` ;
			int $scaleZ	= `checkBoxGrp -q -v3 scaleAxis_checkBox` ;
	
		int $visibilityLock	= `checkBoxGrp -q -v2 visibilityLH_checkBox` ;
		int $visibilityHide	= `checkBoxGrp -q -v3 visibilityLH_checkBox` ;

		string $seleObj[]	= `ls -sl` ;

		//Lock Translate
		if ($translateLock==1)
		 {
			if ($translateX==1)
			 {
				setAttr -lock true ($seleObj[0] + ".translateX") ;
			 }
			if ($translateY==1)
			 {
				setAttr -lock true ($seleObj[0] + ".translateY") ;
			 }
			if ($translateZ==1)
			 {
				setAttr -lock true ($seleObj[0] + ".translateZ") ;
			 }			
		 }

		//Hide Translate
		if ($translateHide==1)
		 {
			if ($translateX==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".translateX") ;
			 }
			if ($translateY==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".translateY") ;
			 }
			if ($translateZ==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".translateZ") ;
			 }
		 }

		//Lock Rotate
		if ($rotateLock==1)
		 {
			if ($rotateX==1)
			 {
				setAttr -lock true ($seleObj[0] + ".rotateX") ;
			 }
			if ($rotateY==1)
			 {
				setAttr -lock true ($seleObj[0] + ".rotateY") ;
			 }
			if ($rotateZ==1)
			 {
				setAttr -lock true ($seleObj[0] + ".rotateZ") ;
			 }			
		 }

		//Hide Rotate
		if ($rotateHide==1)
		 {
			if ($rotateX==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".rotateX") ;
			 }
			if ($rotateY==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".rotateY") ;
			 }
			if ($rotateZ==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".rotateZ") ;
			 }
		 }

		//Lock scale
		if ($scaleLock==1)
		 {
			if ($scaleX==1)
			 {
				setAttr -lock true ($seleObj[0] + ".scaleX") ;
			 }
			if ($scaleY==1)
			 {
				setAttr -lock true ($seleObj[0] + ".scaleY") ;
			 }
			if ($scaleZ==1)
			 {
				setAttr -lock true ($seleObj[0] + ".scaleZ") ;
			 }			
		 }

		//Hide scale
		if ($scaleHide==1)
		 {
			if ($scaleX==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".scaleX") ;
			 }
			if ($scaleY==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".scaleY") ;
			 }
			if ($scaleZ==1)
			 {
				setAttr -keyable false ($seleObj[0] + ".scaleZ") ;
			 }
		 }

		//Lock Visibility
		if ($visibilityLock==1)
		 {
			setAttr -lock true ($seleObj[0] + ".visibility") ;
		 }

		//Hide Visibility
		if ($visibilityHide==1)
		 {
			setAttr -keyable false ($seleObj[0] + ".visibility") ;
		 }
	 }
	//End Lock And Hide Switch Start//////////////////////////////////////////////////
	//////////////////////////////////////////////////////////////////////////////////


	//////////////////////////////////////////////////////////////////////////////////
	//Color Apply/////////////////////////////////////////////////////////////////////
	global proc colorApply_b()
	 {
		int $currentColor		= `colorIndexSliderGrp  -q -value shapeColore_slider` ;	
		string $seleShape[]		= `ls -sl` ;
		int $seleSize			= `size $seleShape` ;

		if ($currentColor>0)
		 {
			int $i ;
			for ($i=0; $i<$seleSize; $i++)
			 {
      				setAttr ($seleShape[$i] + ".overrideEnabled") 1 ;
      				setAttr ($seleShape[$i] + ".overrideColor") ($currentColor-1) ;

				string $controlShape[]	= `listRelatives -s $seleShape[$i]` ;
				int $nodeSize			= `size $controlShape` ;

				int $n ;
				for ($n=0; $n<$nodeSize; $n++)
				 { 
					setAttr ($controlShape[$n] + ".overrideEnabled") 1 ;
    					setAttr ($controlShape[$n] + ".overrideColor") ($currentColor-1) ;
				 }
			 }
		 }
		else if ($currentColor==0)
		 {
			print (".....Please select the color......................." + "\n") ;
		 }	
	 }	
	//End Color Apply/////////////////////////////////////////////////////////////////
