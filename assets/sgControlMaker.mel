	//////////////////////////////////////////////////////////////////////////////////
	//Lock And Hide Check Box Auto Settings //////////////////////////////////////////
	global proc translateAllOnSwitch ()
	 {
		checkBoxGrp -e -v2 1 translateLH_checkBox ;
		checkBoxGrp -e -v3 1 translateLH_checkBox ;		
		checkBoxGrp -e -v1 1 translateAxis_checkBox ;		
		checkBoxGrp -e -v2 1 translateAxis_checkBox ;		
		checkBoxGrp -e -v3 1 translateAxis_checkBox ;	
	 }

	global proc translateAllOfSwitch ()
	 {
		checkBoxGrp -e -v2 0 translateLH_checkBox ;
		checkBoxGrp -e -v3 0 translateLH_checkBox ;		
		checkBoxGrp -e -v1 0 translateAxis_checkBox ;		
		checkBoxGrp -e -v2 0 translateAxis_checkBox ;		
		checkBoxGrp -e -v3 0 translateAxis_checkBox ;	
	 }

	global proc rotateAllOnSwitch ()
	 {
		checkBoxGrp -e -v2 1 rotateLH_checkBox ;
		checkBoxGrp -e -v3 1 rotateLH_checkBox ;		
		checkBoxGrp -e -v1 1 rotateAxis_checkBox ;		
		checkBoxGrp -e -v2 1 rotateAxis_checkBox ;		
		checkBoxGrp -e -v3 1 rotateAxis_checkBox ;	
	 }

	global proc rotateAllOfSwitch ()
	 {
		checkBoxGrp -e -v2 0 rotateLH_checkBox ;
		checkBoxGrp -e -v3 0 rotateLH_checkBox ;		
		checkBoxGrp -e -v1 0 rotateAxis_checkBox ;		
		checkBoxGrp -e -v2 0 rotateAxis_checkBox ;		
		checkBoxGrp -e -v3 0 rotateAxis_checkBox ;	
	 }

	global proc scaleAllOnSwitch ()
	 {
		checkBoxGrp -e -v2 1 scaleLH_checkBox ;
		checkBoxGrp -e -v3 1 scaleLH_checkBox ;		
		checkBoxGrp -e -v1 1 scaleAxis_checkBox ;		
		checkBoxGrp -e -v2 1 scaleAxis_checkBox ;		
		checkBoxGrp -e -v3 1 scaleAxis_checkBox ;	
	 }

	global proc scaleAllOfSwitch ()
	 {
		checkBoxGrp -e -v2 0 scaleLH_checkBox ;
		checkBoxGrp -e -v3 0 scaleLH_checkBox ;		
		checkBoxGrp -e -v1 0 scaleAxis_checkBox ;		
		checkBoxGrp -e -v2 0 scaleAxis_checkBox ;		
		checkBoxGrp -e -v3 0 scaleAxis_checkBox ;	
	 }

	global proc visibilityOnSwitch()
	 {
		checkBoxGrp -e -v2 1 visibilityLH_checkBox ;
		checkBoxGrp -e -v3 1 visibilityLH_checkBox ;		
	 }

	global proc visibilityOfSwitch ()
	 {
		checkBoxGrp -e -v2 0 visibilityLH_checkBox ;
		checkBoxGrp -e -v3 0 visibilityLH_checkBox ;		
	 }
	//End Lock And Hide Check Box Auto Settings //////////////////////////////////////
	//////////////////////////////////////////////////////////////////////////////////


	//////////////////////////////////////////////////////////////////////////////////
	//Connection Type Check Box Auto Settings Start///////////////////////////////////
	global proc parentConstOnSwitch ()
	 {
		checkBoxGrp -e -v3 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v1 0 connectionTypeB_checkBoxGrp ; 
		checkBoxGrp -e -v2 0 connectionTypeB_checkBoxGrp ; 
		checkBoxGrp -e -v3 0 connectionTypeB_checkBoxGrp ; 
	 }

	global proc scaleConstOnSwitch ()
	 {
		checkBoxGrp -e -v1 0 connectionTypeB_checkBoxGrp ; 
		checkBoxGrp -e -v2 0 connectionTypeB_checkBoxGrp ; 
	 }

	global proc pointConstOnSwitch ()
	 {
		checkBoxGrp -e -v1 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v1 0 connectionTypeB_checkBoxGrp ; 
		checkBoxGrp -e -v2 0 connectionTypeB_checkBoxGrp ; 
	 }

	global proc parentingOnSwitch ()
	 {
		checkBoxGrp -e -v1 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v2 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v3 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v2 0 connectionTypeB_checkBoxGrp ; 
		checkBoxGrp -e -v3 0 connectionTypeB_checkBoxGrp ; 
	 }

	global proc connectionOnSwitch ()
	 {
		checkBoxGrp -e -v1 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v2 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v3 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v1 0 connectionTypeB_checkBoxGrp ; 
		checkBoxGrp -e -v3 0 connectionTypeB_checkBoxGrp ; 
	 }

	global proc orientConstOnSwitch ()
	 {
		checkBoxGrp -e -v1 0 connectionTypeA_checkBoxGrp ; 
		checkBoxGrp -e -v1 0 connectionTypeB_checkBoxGrp ;
		checkBoxGrp -e -v1 0 connectionTypeB_checkBoxGrp ;  
		checkBoxGrp -e -v2 0 connectionTypeB_checkBoxGrp ; 
	 }
	//End/////////////////////////////////////////////////////////////////////////////
	//////////////////////////////////////////////////////////////////////////////////

	//////////////////////////////////////////////////////////////////////////////////
	//Rig Type Check Box Auto Settings Start//////////////////////////////////////////
	global proc fkRigOnSwitch ()
	 {
		checkBoxGrp -e -v3 0 rigType_checkBox ; 
	 }

	global proc singleRigOnSwitch ()
	 {
		checkBoxGrp -e -v2 0 rigType_checkBox ; 
	 }
