	//////////////////////////////////////////////////////////////////////////////////
	//GUI/////////////////////////////////////////////////////////////////////////////
	global proc sgControlMaker ()
	 {
		if (`window -q -exists sgControlMakerW`) 
		deleteUI sgControlMakerW ;	

		string $sgControlMakerWin		= `window	-title "SG Control Maker"
									-iconName "Short Name"
									-widthHeight 435 435
									-menuBar true
									sgControlMakerW` ;

		//	showWindow $sgControlMakerWin ;
	
		menu -label "File" -tearOff true ;
   		menuItem -label "New" ;
  		menuItem -label "Open" ;
  		menuItem -label "Save" ;
    		menuItem -divider true ;
    		menuItem -label "Quit" ;
		menu -label "Help" -helpMenu true ;
    		menuItem -label "About Application..." ;

		string $font[]		= {"boldLabelFont", "smallBoldLabelFont", "tinyBoldLabelFont", "plainLabelFont", "smallPlainLabelFont", "obliqueLabelFont", "smallObliqueLabelFont", "fixedWidthFont", "smallFixedWidthFont"} ; 
		float $butC[]	= {0.8, 0.8, 0.8} ;

